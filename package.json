{
  "name": "angular2-tables-api-server",
  "version": "1.0.0",
  "description": "Api server for the Angular2 Tables evaluation app",
  "main": "./lib/server.js",
  "scripts": {
    "test": "mocha --recursive --ui tdd --compilers js:babel-core/register",
    "test:debug": "node-debug _mocha --recursive --ui tdd --compilers js:babel-core/register",
    "test:cover": "nyc --reporter=lcov npm run test",
    "clean": "rm -rf lib",
    "build": "npm run clean && babel src -d lib",
    "build:dev": "npm run clean && babel src -d lib --source-maps \"both\"",
    "start": "npm run build && node ./bin/www",
    "flow": "flow",
    "flow:stop": "flow stop"
  },
  "repository": {
    "type": "git",
    "url": "github.com/DevWurm/angular2-tables-api-server.git"
  },
  "keywords": [
    "API",
    "Rest",
    "Server"
  ],
  "author": "DevWurm",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "flow-bin": "^0.30.0",
    "mocha": "^2.5.3",
    "nyc": "^7.1.0-candidate3"
  },
  "dependencies": {
    "cors": "^2.7.1",
    "dateformat": "^1.0.12",
    "elasticsearch": "^11.0.1",
    "express": "^4.14.0",
    "mongodb": "^2.2.4"
  }
}
